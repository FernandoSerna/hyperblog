Private Sub lv_referencias_ItemClick(ByVal Item As MSComctlLib.ListItem)
    Set lv_referencias.SelectedItem = Item
        pro_textos
        var_modifica_registro = True
        txt_referencias(0).Enabled = True
End Sub

Private Sub tool_atras_siguiente_ButtonClick(ByVal Button As MSComctlLib.Button)
On Error GoTo err0:
   If Button.Index = 2 Or Button.Index = 3 Then
      lv_referencias.SetFocus
      Call pro_avanzar(Me, lv_referencias, Button)
      pro_textos
   End If
   If Button.Index = 1 Then
      lv_referencias.ListItems(1).Selected = True
      pro_textos
   End If
   If Button.Index = 4 Then
      lv_referencias.ListItems(numero_items_referencias).Selected = True
      pro_textos
   End If
err0:
End Sub

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
    Select Case Button.Index
    Case 1
        Call pro_limpiatextos(Me)
        txt_referencias(0).Enabled = True
        txt_referencias(0).SetFocus: var_modifica_registro = False
        Toolbar1.Buttons.Item(2).Enabled = True
        Toolbar1.Buttons.Item(3).Enabled = True
    Case 2
        var_resultado = InStr(1, var_menus, Me.Caption)
        var_inicio = var_resultado + Len(Me.Caption) + 3
        If Mid(var_menus, var_inicio, 1) = "1" Then
            Set var_forma = frmreferencias
            var_swpassword = True
            sw_primera_validacion = False
            frmpasswords.Show 1
        Else
            If Mid(var_menus, var_inicio, 2) = "01" Then
                Set var_forma = frmreferencias
                var_swpassword = True
                sw_primera_validacion = False
                frmpasswords2.txt_supervisor = var_supervisor
                frmpasswords2.Show 1
            Else
               Call pro_guardar_referencias
               rs.Open "select * from tb_referencias", cnn, adOpenDynamic, adLockOptimistic
               If rs.BOF Then
                  Toolbar1.Buttons.Item(2).Enabled = False
                  Toolbar1.Buttons.Item(3).Enabled = False
                  Toolbar1.Buttons.Item(4).Enabled = False
               Else
                  Toolbar1.Buttons.Item(2).Enabled = True
                  Toolbar1.Buttons.Item(3).Enabled = True
                  Toolbar1.Buttons.Item(4).Enabled = True
               End If
               rs.Close
            End If
        End If
    Case 3
       Call pro_textos
    Case 4
        var_resultado = InStr(1, var_menus, Me.Caption)
        var_inicio = var_resultado + Len(Me.Caption) + 3
        If Mid(var_menus, var_inicio, 1) = "1" Then
            Set var_forma = frmreferencias
            var_swpassword = True
            sw_primera_validacion = False
            frmpasswords.Show 1
        Else
            If Mid(var_menus, var_inicio, 2) = "01" Then
                Set var_forma = frmreferencias
                var_swpassword = True
                sw_primera_validacion = False
                frmpasswords2.txt_supervisor = var_supervisor
                frmpasswords2.Show 1
            Else
               Call pro_elimina_referencias
               rs.Open "select * from tb_referencias", cnn, adOpenDynamic, adLockOptimistic
               If rs.BOF Then
                  Toolbar1.Buttons.Item(2).Enabled = False
                  Toolbar1.Buttons.Item(3).Enabled = False
                  Toolbar1.Buttons.Item(4).Enabled = False
               Else
                  Toolbar1.Buttons.Item(2).Enabled = True
                  Toolbar1.Buttons.Item(3).Enabled = True
                  Toolbar1.Buttons.Item(4).Enabled = True
               End If
               rs.Close
            End If
        End If
    Case 6
        If vector_valida_passwords(var_indice_menu) = "*" Then
           frmpasswords.Show
        Else
           Call gPrintListView(lv_referencias, "LISTADO DE referencias")
        End If
    Case 8
        Unload Me
    End Select

End Sub

Sub pro_guardar_referencias()
   Dim ok As Boolean
   Set TB_REFERENCIAS = New TB_REFERENCIAS
   Set TB_BITACORA_REFERENCIAS = New TB_BITACORA_REFERENCIAS
   If txt_referencias(0) <> "" And txt_referencias(1) <> "" Then
      If var_hubo_cambios Then
         rs.Open "select * from tb_referencias where vcha_REF_REFERENCIA_id = '" + txt_referencias(0) + "'", cnn, adOpenDynamic, adLockOptimistic
         ok = TB_REFERENCIAS.Anadir(txt_referencias(0), txt_referencias(1), txt_referencias(2), txt_referencias(3), txt_referencias(4), txt_referencias(5), txt_referencias(6), txt_referencias(7), txt_referencias(8), txt_referencias(9), txt_referencias(10), txt_referencias(11), txt_referencias(12), txt_referencias(13), txt_referencias(14), txt_referencias(15))
         If ok Then
            bitacora = True
            If var_modifica_registro = False Then
               var_operacion_bitacora = "I"
               bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_NOMBRE", var_operacion_bitacora, "", txt_referencias(1), fun_NombreUsuario, fun_NombrePc, Date)
            Else
               var_operacion_bitacora = "M"
               If rs(0) <> txt_referencias(0) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_REFERENCIA_ID", var_operacion_bitacora, rs(0), txt_referencias(0), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If rs(1) <> txt_referencias(1) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_NOMBRE", var_operacion_bitacora, rs(1), txt_referencias(1), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If Trim(rs(2)) <> Trim(txt_referencias(2)) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_TITULO", var_operacion_bitacora, rs(2), txt_referencias(2), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If rs(3) <> txt_referencias(3) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_TABLA_NOMBRE", var_operacion_bitacora, rs(3), txt_referencias(3), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If rs(4) <> txt_referencias(4) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_TABLA_FISICO", var_operacion_bitacora, rs(4), txt_referencias(4), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If rs(5) <> txt_referencias(5) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_CAMPO_REFERENCIA", var_operacion_bitacora, rs(5), txt_referencias(5), fun_NombreUsuario, fun_NombrePc, Date)
               End If
               If rs(6) <> txt_referencias(6) Then
                  bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_CAMPO_CANTIDAD", var_operacion_bitacora, rs(6), txt_referencias(6), fun_NombreUsuario, fun_NombrePc, Date)
               End If
            End If
            rs.Close
            pro_actualiza_ListView
            txt_referencias(0).Enabled = False
            MsgBox "Informacion Guardada Correctamente ! ", vbOKOnly + vbInformation, "Aviso"
            txt_registros = lv_referencias.ListItems.Count
            var_modifica_registro = True
         Else
            MsgBox "No se puede grabar registro: " + TB_REFERENCIAS.MensajeError, vbOKOnly + vbCritical, "ATENCION"
         End If
      End If
   End If
   Set TB_REFERENCIAS = Nothing: var_hubo_cambios = False
End Sub

Sub pro_elimina_referencias()
Dim var_llave_usuarios As String

Set TB_REFERENCIAS = New TB_REFERENCIAS
Set TB_BITACORA_REFERENCIAS = New TB_BITACORA_REFERENCIAS
On Error GoTo SALIR:
   ok = True
   If txt_referencias(0) <> "" And txt_referencias(1) <> "" And var_modifica_registro = True Then
      If MsgBox("Desea Eliminar este Registro", vbInformation + vbYesNo, "Aviso") = vbYes Then
         ok = TB_REFERENCIAS.Eliminar(txt_referencias(0))
      Else
         GoTo SALIR:
      End If
      If ok Then
         var_operacion_bitacora = "E"
         bitacora = TB_BITACORA_REFERENCIAS.Anadir(txt_referencias(0), "VCHA_REF_NOMBRE", var_operacion_bitacora, txt_referencias(1), "", fun_NombreUsuario, fun_NombrePc, Date)
         numero_items_referencias = numero_items_referencias - 1
         MsgBox "Se Elimino Correctamente el Registro", vbInformation
         lv_referencias.ListItems.Remove (lv_referencias.SelectedItem.Index)
         Call pro_limpiatextos(Me)
         txt_registros = lv_referencias.ListItems.Count
         lv_referencias.SelectedItem.Selected = True
         pro_textos
      Else
         MsgBox "No se puede grabar registro: " + TB_REFERENCIAS.MensajeError, vbOKOnly + vbCritical, "ATENCION"
      End If
   End If
SALIR:
Set TB_REFERENCIAS = Nothing
End Sub


