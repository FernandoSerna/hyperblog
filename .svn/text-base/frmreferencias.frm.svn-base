Private Sub cmd_buscar_Click()
    Call pro_busca_registro(lv_referencias, txt_buscar, False)
    txt_buscar = ""
    pro_textos

End Sub

Private Sub cmbreferencias_Change()
   If cmbreferencias = "POSITIVA" Then
      txt_referencias(2) = "+"
   End If
   If cmbreferencias = "NEGATIVA" Then
      txt_referencias(2) = "-"
   End If
   If cmbreferencias = "TRASPASO" Then
      txt_referencias(2) = "T"
   End If
End Sub

Private Sub cmbreferencias_Click()
   If cmbreferencias = "POSITIVA" Then
      txt_referencias(2) = "+"
   End If
   If cmbreferencias = "NEGATIVA" Then
      txt_referencias(2) = "-"
   End If
   If cmbreferencias = "TRASPASO" Then
      txt_referencias(2) = "T"
   End If
End Sub

Private Sub cmbreferencias_KeyPress(KeyAscii As Integer)
   KeyAscii = 0
End Sub

Private Sub Combo1_Click()
    txt_referencias(4) = Obtener_llave(cnn, rs, "TB_REFERENCIAS", "VCHA_REF_REFERENCIA_ID", Combo1, 0, "T")
End Sub

Private Sub Combo2_Click()
   txt_referencias(1) = Obtener_llave(cnn, rs, "TB_ESTADOS", "VCHA_EST_NOMBRE", Combo2, 1, "T")
   
End Sub

Private Sub Combo2_KeyPress(KeyAscii As Integer)
   If KeyAscii = 13 Then
      txt_referencias(2).SetFocus
   Else
      KeyAscii = 0
   End If
End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)
   Call pro_enfoque(KeyAscii)
End Sub

Private Sub Form_Load()
    var_modifica_registro = True
    lv_referencias.SmallIcons = ImageList1
    Call pro_encabezadosView(Me, lv_referencias, False)
    Call pro_llena_listview1
    pro_textos

    Call pro_AsignarAViewColor(lv_referencias, Picture1, vbWhite, vbGray)
    rs.Open "select * from tb_referencias", cnn, adOpenDynamic, adLockOptimistic
    If rs.BOF Then
       Toolbar1.Buttons.Item(2).Enabled = False
       Toolbar1.Buttons.Item(3).Enabled = False
       Toolbar1.Buttons.Item(4).Enabled = False
    Else
       Toolbar1.Buttons.Item(2).Enabled = True
       Toolbar1.Buttons.Item(3).Enabled = True
       Toolbar1.Buttons.Item(4).Enabled = True
    End If
    rs.Close
End Sub

